{
    "npmClient": "yarn",
    "packages": [
        "token-metadata/js",
        "demo"
    ],
    "useWorkspaces": true,
    "version": "independent",
    "command": {
        "version": {
            "allowBranch": [
                "master"
            ],
            "message": "chore: version updated packages",
            "ignoreChanges": [
                "**/test/**",
                "**/*.md"
            ]
        },
        "publish": {
            "registry": "https://registry.npmjs.org/"
        }
    }
}